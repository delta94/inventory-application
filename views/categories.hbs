<h2>{{title}}</h2>
<section class="category-list">
    {{#each category_list}}
    <figure>
        <a href="{{url}}">
            <img src="{{file_url}}" alt="">
            <figcaption>
                <p class="cat-title">{{name}}</p>
                <p class="cat-desc">&mdash; {{description}}.</p>
            </figcaption>
        </a>
    </figure>
    {{/each}}
</section>
<footer>
    <p>Made by Julio Bermudez <a href="https://github.com/julio22b"><img src="/images/iconmonstr-github-1-24.png" alt="github icon"></a></p>
</footer>